#include <stdio.h>

int main(void) {
  int j, i,n,a[50],key;
 printf("enter the number of elements\n");
 scanf("%d\n",&n);
 for(i=0;i<n;i++)
 {
   scanf("%d",&a[i]);
 }
for(i=0;i<n;i++){
for(j=0;j<n-i;j++)
{
  if(a[j]>a[j+1]){
	  key=a[j];
	  a[j]=a[j+1];
	  a[j+1]=key;
  }

}
}
for(i=0;i<n;i++)
 {
   printf("%d\t",a[i]);
 }

}
